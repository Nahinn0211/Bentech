{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/assets/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/assets/js/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/assets/images/$1"
    },
    {
      "src": "/(.*\\.(png|jpg|jpeg|svg|ico|webp|woff|woff2|ttf|eot|webmanifest))",
      "dest": "/assets/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}